<h1>Editing match</h1>

<% form_for(@match) do |f| %>
  <%= error_messages_for :match %>

  <%= render :partial => 'form', :locals => {:f => f} %>

  <div style="clear:left;" class="button-section">
    <%= f.submit "Update" %>
    <%= link_to 'Show', @match %> |
    <%= link_to 'Back', matches_path %>
  </div>
<% end %>
